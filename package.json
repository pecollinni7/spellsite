    "auto-updater": "^1.0.2",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.2.2",
    "file-type": "^14.1.3",
    "jquery": "^3.4.1",
    "muuri": "^0.8.0",
    "needle": "^2.3.2",
    "slash": "^3.0.0"
