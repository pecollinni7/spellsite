{
  "name": "spellsite",
  "version": "1.6.2",
  "description": "Electron SpellSite application",
  "main": "main.js",
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "pecollinni7",
        "repo": "spellsite"
      }
    ]
  },
  "scripts": {
