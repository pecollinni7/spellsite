{
  "name": "spellsite",
  "version": "1.6.3",
  "description": "Electron SpellSite application",
  "main": "main.js",
  "build": {
    "win": {
      "publish": [
        "github",
        "bintray"
      ]
    },
    "appId": "com.example.app",
    "publish": [
      {
        "provider": "github",
        "owner": "pecollinni7",
        "repo": "spellsite"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "build": "build",
    "shipit": "build -p always",
    "releasepatch": "npm version patch && pack -p always",
    "releaseminor": "npm version minor && build -p always",
    "releasemajor": "npm version major && build -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pecollinni7/spellsite.git"
  },
  "keywords": [
    "electron",
    "example",
    "autoUpdater",
    "update",
    "github"
  ],
  "author": "Petar Jevtic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pecollinni7/spellsite/issues"
  },
  "homepage": "https://github.com/pecollinni7/spellsite#readme",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.2",
    "auto-updater": "^1.0.2",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.2.2",
    "file-type": "^14.1.3",
    "image-size": "^0.9.3",
    "jquery": "^3.5.0",
    "muuri": "^0.8.0",
    "needle": "^2.3.2",
    "slash": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.9.1"
  }
}
